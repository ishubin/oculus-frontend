<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


    <bean name="/create" class="net.mindengine.oculus.frontend.web.controllers.chart.CreateChartController">
        <property name="chartDAO" ref="chartDAO"></property>
    </bean>
    
    <bean name="/display" class="net.mindengine.oculus.frontend.web.controllers.chart.DisplayChartController">
        <property name="chartDAO" ref="chartDAO"></property>
    </bean>
    
</beans>