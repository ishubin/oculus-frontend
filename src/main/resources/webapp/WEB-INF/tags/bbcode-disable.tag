<%@ tag body-content="scriptless" %><%@ taglib uri="http://www.springframework.org/tags" prefix="spring" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><jsp:doBody var="body"/><%String text = (String) jspContext.findAttribute("body");if(text!=null){    text = text.replaceAll("\\[b\\](.*?)\\[/b\\]", "$1");    text = text.replaceAll("\\[i\\](.*?)\\[/i\\]", "$1");    text = text.replaceAll("\\[u\\](.*?)\\[/u\\]", "$1");    text = text.replace("[ul]","");    text = text.replace("[/ul]","");    text = text.replace("[li]","");    text = text.replace("[/li]","");    text = text.replaceAll("\\[url\\](.*?)\\[/url\\]", "$1");    text = text.replaceAll("\\[url=(.*?)\\](.*?)\\[/url\\]", "$2");    text = text.replaceAll("\\[size=?([0-9]|[1-2][0-9])?\\](.*?)\\[/size\\]","$2");    text = text.replaceAll("\\[color=(.*?)\\](.*?)\\[/color\\]","$2");    text = text.replaceAll("\\[img\\](.*?)\\[/img\\]", "");    text = text.replaceAll("\\[parameter\\](.*?)\\[/parameter\\]", "$1");    out.print(text);}%>