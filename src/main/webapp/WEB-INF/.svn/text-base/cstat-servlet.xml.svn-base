    <bean name="/create" class="net.mind_engine.oculus.web.controllers.cstat.CreateCustomStatisticController">
        <property name="title" value="Create Custom Statistic"/>
        <property name="sessionForm" value="true"/>
        <property name="commandName" value="customStatistic"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.customstatistics.CustomStatistic"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.customstatistics.CustomStatisticValidator">
            </bean>
        </property>
        <property name="formView" value="cstat-create"/>
        <property name="projectDAO" ref="projectDAO"/>
        <property name="authorizationCheck" value="true"/>
    	<property name="customStatisticDAO" ref="customStatisticDAO"/>
    	<property name="permissions">
            <list>
                <ref bean="_P_CSTAT_MANAGMENT"/>
            </list>
        </property>
    </bean>
    
    
