<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


    <bean name="/home" class="net.mindengine.oculus.frontend.web.controllers.display.HomeController">
        <property name="view" value="home"/>
        <property name="title" value="Home"/>
    </bean>
    
    <bean name="/file" class="net.mindengine.oculus.frontend.web.controllers.display.FileDisplayController">
        <property name="config" ref="oculusConfig"></property>
    </bean>
    
</beans>