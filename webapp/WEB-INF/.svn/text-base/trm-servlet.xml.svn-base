<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

   
    <bean id="taskColumnFactory" class="net.mind_engine.oculus.db.search.ColumnFactory">
        <property name="columnClass" value="net.mind_engine.oculus.domain.trm.TaskSearchColumn"/>
        <property name="file" value="WEB-INF/conf/task-columns.xml"/>
    </bean>
    
    <bean name="/main" class="net.mind_engine.oculus.web.controllers.SecureSimpleViewController">
        <property name="view" value="trm-main-page"/>
    </bean>
    
    <bean name="/customize" class="net.mind_engine.oculus.web.controllers.SecureSimpleViewController">
        <property name="view" value="trm-customize-page"/>
        <property name="authorizationCheck" value="true"/>
        <property name="permissions">
            <list>
                <ref bean="_P_TRM_ADMINISTRATION"/>
            </list>
        </property>
    </bean>
    
    
    <bean name="/customize-suite-parameters" class="net.mind_engine.oculus.web.controllers.trm.customize.CustomizeSuiteParametersController">
        <property name="view" value="trm-customize-suite-parameters"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
        <property name="authorizationCheck" value="true"/>
        <property name="permissions">
            <list>
                <ref bean="_P_TRM_ADMINISTRATION"/>
            </list>
        </property>
    </bean>
    
    
    <bean name="/my-tasks" class="net.mind_engine.oculus.web.controllers.trm.tasks.MyTasksController">
        <property name="view" value="trm-my-tasks"/>
        <property name="authorizationCheck" value="true"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/my-active-tasks" class="net.mind_engine.oculus.web.controllers.trm.tasks.MyActiveTasksController">
        <property name="view" value="trm-my-active-tasks"/>
        <property name="authorizationCheck" value="true"/>
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/create-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.CreateTaskController">
        <property name="sessionForm" value="true"/>
        <property name="commandName" value="task"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TrmTask"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.trm.task.CreateTaskValidator"></bean>
        </property>
        <property name="formView" value="trm-create-task"/>
        <property name="authorizationCheck" value="true"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
    </bean>
    
    <bean name="/delete-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.DeleteTaskController">
        <property name="view" value="redirect:../test-run-manager/my-tasks"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/edit-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.EditTaskController">
        <property name="view" value="trm-edit-task"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/create-suite" class="net.mind_engine.oculus.web.controllers.trm.tasks.CreateSuiteController">
        <property name="sessionForm" value="true"/>
        <property name="commandName" value="suite"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TrmSuite"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.trm.task.CreateSuiteValidator"></bean>
        </property>
        <property name="formView" value="trm-create-suite"/>
        <property name="authorizationCheck" value="true"/>
        <property name="trmDAO" ref="trmDAO"/>
    </bean>
    
    <bean name="/delete-suite" class="net.mind_engine.oculus.web.controllers.trm.tasks.DeleteSuiteController">
        <property name="trmDAO" ref="trmDAO"/>
    </bean>
    <bean name="/edit-suite" class="net.mind_engine.oculus.web.controllers.trm.tasks.EditSuiteController">
        <property name="view" value="trm-edit-suite"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="testDAO" ref="testDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
        <property name="authorizationCheck" value="true"/>
        <property name="customizationDAO" ref="customizationDAO"/>
    </bean>
   
    <bean name="/run-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.RunTaskController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="testDAO" ref="testDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/ajax-fetch-tasks" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxFetchTasks">
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    
    <bean name="/ajax-get-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxGetTaskController">
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/ajax-stop-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxStopTaskController">
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/ajax-remove-completed-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxRemoveCompletedTaskController">
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    <bean name="/scheduler" class="net.mind_engine.oculus.web.controllers.trm.scheduler.SchedulerController">
        <property name="config" ref="oculusConfig"/>
        <property name="view" value="trm-scheduler"/>
    </bean>
    
    <bean name="/scheduler-change-occurrence" class="net.mind_engine.oculus.web.controllers.trm.scheduler.SchedulerChangeOccurrenceController">
        <property name="config" ref="oculusConfig"/>
        <property name="view" value="redirect:../test-run-manager/scheduler"/>
    </bean>
    
    <bean name="/scheduler-delete-task" class="net.mind_engine.oculus.web.controllers.trm.scheduler.SchedulerDeleteTaskController">
        <property name="config" ref="oculusConfig"/>
        <property name="view" value="redirect:../test-run-manager/scheduler"/>
    </bean>
    
    <bean name="/upload-project-choose-project" class="net.mind_engine.oculus.web.controllers.trm.customize.UploadProjectChooseProjectController">
        <property name="projectDAO" ref="projectDAO"/>
        <property name="view" value="trm-upload-project-choose-project"/>
    </bean>
    
    <bean name="/upload-project" class="net.mind_engine.oculus.web.controllers.trm.customize.UploadProjectController">
        <property name="sessionForm" value="true"/>
        <property name="commandName" value="uploadProject"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TrmUploadProject"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.trm.TrmUploadProjectValidator"></bean>
        </property>
        <property name="formView" value="trm-upload-project"/>
        <property name="successView" value="trm-upload-project-success"/>
        <property name="authorizationCheck" value="true"/>
        <property name="projectDAO" ref="projectDAO"/>
        <property name="config" ref="oculusConfig"/>
    </bean>
    
    
    <bean name="/ajax-fetch-suite-parameters" class="net.mind_engine.oculus.web.controllers.trm.customize.AjaxFetchSuiteParametersController">
        <property name="trmDAO" ref="trmDAO"/>
    </bean>
    
    
    <bean name="/agents" class="net.mind_engine.oculus.web.controllers.trm.agent.AgentsDisplayController">
        <property name="config" ref="oculusConfig"/>
        <property name="view" value="trm-agents"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/shared-tasks" class="net.mind_engine.oculus.web.controllers.trm.tasks.SharedTaskSearchController">
        <property name="title" value="Search Tasks"/>
        <property name="sessionForm" value="true"/>
        <property name="commandName" value="searchFilter"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TaskSearchFilter"/>
        <property name="formView" value="search-test"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="userDAO" ref="userDAO"/>
        <property name="successView" value="search-shared-tasks"/>
        <property name="columnFactory" ref="taskColumnFactory"/>
        <property name="config" ref="oculusConfig"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/shared-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.SharedTaskController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="view" value="trm-shared-task"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/copy-shared-task" class="net.mind_engine.oculus.web.controllers.trm.tasks.CopySharedTaskController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/ajax-suite-search" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxSuiteSearchController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/copy-suites" class="net.mind_engine.oculus.web.controllers.trm.tasks.CopySuitesController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    
    
    <bean name="/create-suite-group" class="net.mind_engine.oculus.web.controllers.trm.tasks.CreateSuiteGroupController">
        <property name="sessionForm" value="true"/>
        <property name="title" value="Create Suite Group"/>
        <property name="commandName" value="createSuiteGroup"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TrmSuiteGroup"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.trm.task.CreateSuiteGroupValidator"></bean>
        </property>
        <property name="formView" value="trm-create-suite-group"/>
        <property name="authorizationCheck" value="true"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
    </bean>
    
    
    <bean name="/edit-suite-group" class="net.mind_engine.oculus.web.controllers.trm.tasks.EditSuiteGroupController">
        <property name="sessionForm" value="true"/>
        <property name="title" value="Create Suite Group"/>
        <property name="commandName" value="editSuiteGroup"/>
        <property name="commandClass" value="net.mind_engine.oculus.domain.trm.TrmSuiteGroup"/>
        <property name="validator">
            <bean class="net.mind_engine.oculus.service.trm.task.CreateSuiteGroupValidator"></bean>
        </property>
        <property name="formView" value="trm-edit-suite-group"/>
        <property name="authorizationCheck" value="true"/>
        <property name="trmDAO" ref="trmDAO"/>
        <property name="projectDAO" ref="projectDAO"/>
    </bean>
    
    <bean name="/delete-suite-group" class="net.mind_engine.oculus.web.controllers.trm.tasks.DeleteSuiteGroupController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/ajax-my-active-tasks" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxMyActiveTasksController">
        <property name="authorizationCheck" value="true"/>
        <property name="config" ref="oculusConfig"/>
        <property name="view" value="trm-ajax-my-active-tasks"/>
    </bean>
    
    <bean name="/ajax-task-search" class="net.mind_engine.oculus.web.controllers.trm.tasks.AjaxTaskSearchController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/add-task-dependencies" class="net.mind_engine.oculus.web.controllers.trm.tasks.AddTaskDependenciesController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    <bean name="/delete-task-dependencies" class="net.mind_engine.oculus.web.controllers.trm.tasks.DeleteTaskDependenciesController">
        <property name="trmDAO" ref="trmDAO"/>
        <property name="authorizationCheck" value="true"/>
    </bean>
    
    
    
</beans>